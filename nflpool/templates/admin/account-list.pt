<div metal:use-macro="view.layout">
    <div metal:fill-slot="main_content">


        <div class="container">
            <div class="row upcoming">
                <div class="col-md-3"></div>
                <div class="col-md-6">


                    <h1>Account Management</h1>

                        <table class="table table-striped">
                        <thead>
                            <tr>
                                <th>First Name</th>
                                <th>Last Name</th>
                                <th>Created Date</th>
                                <th>Email</th>
                                <th>Twitter</th>
                            </tr>
                        </thead>
                            <tbody>
                            <tr tal:repeat="a account_list">
                                <td>${a.first_name}</td>
                                <td>${a.last_name}</td>
                                <td>${a.created}</td>
                                <td>${a.email}</td>
                                <td>${a.twitter}</td>
                            </tr>
                            </tbody>
                        </table>

                </div>
            </div>
        </div>

    </div>

</div>
